<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient android:startColor="#ce6384ca" android:endColor="#ce6384ca" android:angle="-90.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="3.0dip" />
            <stroke android:width="1.0dip" android:color="#ff5e7974" />
            <solid android:color="#ff58857e" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="12.0dip" />
            <gradient android:startColor="#ee6384ca" android:endColor="#ee6384ca" android:angle="-90.0" />
        </shape>
    </item>
</selector>